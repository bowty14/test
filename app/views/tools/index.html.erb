        <li class="item1 active"></li>
        <li class="item2"></li>
        <li class="item3"></li>
        <li class="item4"></li>
        <li class="item5"></li>
        <li class="item6"></li>
        <li class="item7"></li>
      </ol>

      <!-- Wrapper for slides -->
      
      <div class="carousel-inner" role="listbox">

        <div class="item active">
          <img src="protest1.jpg" class="img-responsive" alt="" style="width:100%; height:200px;" >
        </div>

        <div class="item">
          <img src="protest2.jpg" class="img-responsive" alt="" style="width:100%; height:200px;">
        </div>
      
        <div class="item" >
          <img src="protest8.jpg" class="img-responsive" alt="" style="width:100%; height:200px;" >
